html
  head
    title= title
    style
      include ./style.css
  body
    #main
      h2= title
      #details
        p= `Name: ${scooter.licencePlate}`
        p= `Fuel level: ${scooter.fuelLevel}`
        p= `Address: ${scooter.address}`
      iframe(
        width= mapSize,
        height= mapSize,
        frameborder='0',
        style='border:0'
        src=`https://www.google.com/maps/embed/v1/directions?key=${apiKey}&origin=${homeAddress}+Berlin&destination=${scooter.address}&mode=walking`)
